  console.log(\"Submitted\");\n    } else {\n      alert(\"You are not logged in. Please log in to submit team.\");\n    }\n  };\n\n  console.log(loginPage);\n  \nconst fun = () => { \n  {\n    if (loginPage === true) { \n        return ( \n          <LoginPage signIn={signIn} signOut={signOut} user={user} setLoginPage={setLoginPage} />\n        )\n      } else if (pitchPage === true) { \n        return (\n          <SelectTeamPage currentTeam={currentTeam} addPlayerToTeam={addPlayerToTeam} addToDb={addToDb} user={user}/>\n        )\n      } else { \n        return ( \n          <StatsPage />\n        )\n      }\n    }\n}\n\n  return (\n\n    <Router>\n    <Navbar signIn={signIn} signOut={signOut} user={user} setLoginPage={setLoginPage}/>\n\n    {\n      fun()\n    }\n    {/* {\n      pitchPage ? (<SelectTeamPage currentTeam={currentTeam} addPlayerToTeam={addPlayerToTeam} addToDb={addToDb} user={user}/>) : (<StatsPage />)\n    } */}\n\n    </Router>   \n  );\n};\n\nexport default Routes;\n","/Users/paulglazzard/Code/bears-selector-type/src/containers/TeamScoreModal/index.js",[],"/Users/paulglazzard/Code/bears-selector-type/src/containers/TeamScoreModal/TeamScoreModal.tsx",[],"/Users/paulglazzard/Code/bears-selector-type/src/containers/SignInPage/index.js",[],["198","199"],"/Users/paulglazzard/Code/bears-selector-type/src/containers/SignInPage/SignInPage.tsx",[],["200","201"],"/Users/paulglazzard/Code/bears-selector-type/src/components/InputField/index.js",[],"/Users/paulglazzard/Code/bears-selector-type/src/components/InputField/InputField.tsx",[],"/Users/paulglazzard/Code/bears-selector-type/src/containers/SelectTeamPage/index.js",[],"/Users/paulglazzard/Code/bears-selector-type/src/containers/StatsPage/index.js",[],"/Users/paulglazzard/Code/bears-selector-type/src/containers/LoginPage/index.js",[],"/Users/paulglazzard/Code/bears-selector-type/src/containers/SelectTeamPage/SelectTeamPage.tsx",["202","203","204"],"import React, { useEffect, useState } from \"react\"